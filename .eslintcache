ices}\r\n                setStorePrices={setStorePrices}\r\n                name={\"How Many Pages\"}\r\n              />\r\n            </Route>\r\n\r\n            <Route path=\"/q71additional-func\">\r\n              <Q71AdditionalFunc\r\n                storePrices={storePrices}\r\n                setStorePrices={setStorePrices}\r\n              />\r\n            </Route>\r\n\r\n            <Route path=\"/q72additional-func\">\r\n              <Q72AdditionalFunc\r\n                storePrices={storePrices}\r\n                setStorePrices={setStorePrices}\r\n              />\r\n            </Route>\r\n\r\n            <Route path=\"/contacts-and-details\">\r\n              <Q8ContactAndDetails storePrices={storePrices} />\r\n            </Route>\r\n\r\n            <Route path='/thankyou'>\r\n              <ThankYou />\r\n            </Route>\r\n          </Switch>\r\n        </AnimatePresence>\r\n      </div>\r\n      {/* END Main area */}\r\n      \r\n      {/* Calc area */}\r\n      <div className=\"app-calc\">\r\n        <FontAwesomeIcon\r\n          onClick={goBackHandler}\r\n          className=\"goBack-btn-arrow\"\r\n          icon={faArrowUp}\r\n        />\r\n        <FontAwesomeIcon\r\n          onClick={goForwardHandler}\r\n          className={`goForward-btn-arrow ${lastPage ? 'hideArrow' : ''}`}\r\n          icon={faArrowDown}\r\n        />\r\n\r\n        <div className=\"app-calc-stats\" id={`${contactPage ? 'showCalcArea' : ''}`}>\r\n          <Quote totalPrice={totalPrice} storePrices={storePrices} />\r\n        </div>\r\n      </div>\r\n      {/* END Calc area */}\r\n\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\Ivo\\Desktop\\quick-quote-calculator_for_LittleVitamin\\src\\animation.js",[],"C:\\Users\\Ivo\\Desktop\\quick-quote-calculator_for_LittleVitamin\\src\\components\\Q6-1-ManyProducts\\Q6ManyProducts.jsx",[],"C:\\Users\\Ivo\\Desktop\\quick-quote-calculator_for_LittleVitamin\\src\\components\\Q1LetsStart\\Q1LetsStart.j