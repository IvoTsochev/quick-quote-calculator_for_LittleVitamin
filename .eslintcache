og(result.text);\n            console.log('msg sent');\n            setIsSent(undefined);\n\n          },\n          (error) => {\n            console.log(error.text);\n          }\n        );\n    }, 1500);\n\n  }\n  // END Initiating the contact form\n\n\n  return (\n    <motion.div\n      className=\"contacts-and-details page\"\n      variants={pageAnimation}\n      initial=\"hidden\"\n      animate=\"show\"\n      exit=\"exit\"\n    >\n\n\n\n      {!isSent ? (\n        <div className=\"forms-container\">\n\n\n          <form className=\"contact-form\" onSubmit={sendEmail}>\n            <QuestionTitle>\n              Help us to provide you with a more accurate estimate.\n         </QuestionTitle>\n            <p>\n              Write us a simple site brief and list out any special requiremenents\n              that haven't yet been covered.\n         </p>\n            {/* Textarea */}\n            <textarea\n              className=\"contact-message\"\n              placeholder=\"Message\"\n              name=\"message\"\n              cols=\"90\"\n              rows=\"10\"\n            />\n            <QuestionTitle>\n              Please provide us with some information about you?\n         </QuestionTitle>\n            <div className=\"contact-first-row\">\n              {/* Client preferences passed to the form */}\n              <input type=\"hidden\" name=\"clientPreferences\" value={clientPreferences} />\n\n              {/* Name */}\n              <input\n                className=\"contact-name\"\n                type=\"text\"\n                placeholder=\"Name\"\n                name=\"name\"\n              />\n              {/* Email Address */}\n              <input\n                className=\"contact-email\"\n                type=\"email\"\n                placeholder=\"Email Address\"\n                name=\"email\"\n              />\n            </div>\n\n            <div className=\"contact-second-row\">\n              {/* Phone Number */}\n              <input\n                className=\"telephone-number\"\n                type=\"text\"\n                placeholder=\"Telephone Number\"\n                name=\"phone\"\n              />\n              {/* Country */}\n              <input\n                className=\"contact-country\"\n                type=\"text\"\n                placeholder=\"Country\"\n                name=\"country\"\n              />\n\n              {/* Coupon Code */}\n              <input\n                className=\"contact-coupon\"\n                type=\"text\"\n                placeholder=\"Coupon Code\"\n                name=\"coupon\"\n              />\n            </div>\n\n            <input\n              className=\"contact-submit\"\n              type=\"submit\"\n              value=\"Send\"\n            />\n          </form>\n        </div>\n      ) : (\n          <LoadingSpinner />\n        )}\n\n\n      {isSent && (\n        <Redirect to='/thankyou' />\n      )}\n\n\n\n    </motion.div>\n  );\n};\n\nexport default Q8ContactAndDetails;\n","/Users/ivaylotsochev/Desktop/quick-quote-calculator_for_LittleVitamin/src/components/ThankYou/ThankYou.jsx",[],"/Users/ivaylotsochev/Desktop/quick-quote-calculator_for_LittleVitamin/src/globalStyling.js",[],"/Users/ivaylotsochev/Desktop/quick-quote-calculator_for_LittleVitamin/src/util.js",[],"/Users/ivaylotsochev/Desktop/quick-quote-calculator_for_LittleVitamin/src/components/QuoteListing/QuoteListing.jsx",[],"/Users/ivaylotsochev/Desktop/quick-quote-calculator_for_LittleVitamin/src/components/SingleCheckBtn/SingleCheckBtn.jsx",[],"/Users/ivaylotsochev/Desktop/quick-quote-calculator_for_LittleVitamin/src/components/LoadingSpinner/LoadingSpinner.jsx",[],{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","severity":1,"message":"101","line":2,"column":10,"nodeType":"102","messageId":"103","endLine":2,"endColumn":14},{"ruleId":"100","severity":1,"message":"104","line":121,"column":15,"nodeType":"102","m